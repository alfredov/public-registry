{
  "name": "react-app-template",
  "version": "1.0.1",
  "main": "src/index",
  "license": "MIT",
  "files": [
    "src"
  ],
  "scripts": {
    "webpack-dev-server": "webpack-dev-server",
    "webpack": "webpack",
    "debug": "node --inspect --inspect-brk ./node_modules/webpack/bin/webpack.js",
    "prod": "npm run webpack -- --env.mode production && npm run sh:copy-public-to-build",
    "dev": "npm run webpack-dev-server -- --env.mode development --hot",
    "prod:analyze": "npm run prod -- --env.presets analyze",
    "prod:compress": "npm run prod -- --env.presets compress",
    "prod:debug": "npm run debug -- --env.mode production",
    "dev:debug": "npm run debug -- --env.mode development",
    "lint": "tslint -p ./tsconfig.json",
    "sh:copy-public-to-build": "bash ./tools/bash/copy-public-to-build.sh",
    "build:esnext": "yarn install && mkdir -p ./dist/esnext && rm -rf ./dist/esnext && tsc -p ./src/core/config/tsconfig.esnext.json"
  },
  "gitpkg": {
    "registry": "https://github.com/alfredov/public-registry.git"
  },
  "repository": "https://github.com/alfredov/react-app-template.git",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@types/node": "^14.14.16",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.14",
    "@types/react-router-dom": "^5.1.6",
    "babel-loader": "^8.2.2",
    "compression-webpack-plugin": "^7.1.1",
    "css-loader": "^5.0.1",
    "gitpkg": "^1.0.0-beta.2",
    "html-webpack-plugin": "^4.5.0",
    "mini-css-extract-plugin": "^1.3.3",
    "redux-logger": "^3.0.6",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.12",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3",
    "webpack": "4.8.0",
    "webpack-bundle-analyzer": "^4.3.0",
    "webpack-cli": "2.1.3",
    "webpack-dev-server": "3.1.4",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "@emotion/react": "^11.1.4",
    "connected-react-router": "^6.8.0",
    "conventional-changelog-cli": "^2.1.1",
    "format-message": "^6.2.3",
    "format-message-generate-id": "^6.2.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-observable": "^1.2.0",
    "rxjs": "^6.6.3",
    "typesafe-actions": "^5.1.0"
  }
}
